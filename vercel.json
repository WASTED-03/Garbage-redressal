{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": [
        "views/**",
        "uploads/**",
        "asset/**"
      ]
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "api/index.js" }
  ]
}


